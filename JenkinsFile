                echo "checkout proj"
                //checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/madhursk/Spring-boot_App']]])
            }
            
        }
        
        stage('Build'){
            steps{
                echo "Building Project"
            }
        }
    }
}
